{
  "name": "playwright",
  "version": "1.0.0",
  "description": "playwright-automation",
  "main": "index.js",
  "scripts": {
    "test:chrome": "playwright test --headed --update-snapshots --project=Chrome --workers 1",
    "test:firefox": "playwright test --headed --update-snapshots --project=Firefox --workers 1",
    "test:all": "playwright test --headed --update-snapshots",
    "test:headless:chrome": "playwright test --update-snapshots --project=Chrome --workers 1",
    "test:headless:firefox": "playwright test --update-snapshots --project=Firefox --workers 1",
    "test:headless:all": "playwright test --update-snapshots",
    "lint": "node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olegstat/playwright-demo.git"
  },
